<section>
  <%= link_to "Cadastrar Nova Categoria", new_category_path, class:"btn btn-primary" %>
  <h2>Lista de Categorias</h2>

  <% @categories.each do |category| %>
    <ul class="list">
      <li>
        <%= category.name %>
        <%= link_to "Editar", edit_category_path(category)  %>
      </li>
    </ul>
  <% end %>
</section>
